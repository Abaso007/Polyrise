{
  "name": "Alpine.js Text Animation Demo",
  "version": "0.0.1",
  "title": "Alpine.js Text Animation Demo",
  "description": "Alpine.js Text Animation Demo",
  "author": "Polyrise",
  "url": "https://michaelsboost.com/",
  "meta": "",
  "libraries": [
    "https://fonts.googleapis.com/css2?family=Noto+Sans:wght@400;700&amp;display=swap",
    "https://michaelsboost.com/TailwindCSSMod/tailwind-mod.min.js",
    "https://cdnjs.cloudflare.com/ajax/libs/alpinejs/3.14.1/cdn.js"
  ],
  "css": {
    "rootVariables": {},
    "styles": {},
    "animations": {},
    "breakpoints": {}
  },
  "components": [],
  "html": [
    {
      "tag": "div",
      "id": "w9alymr5m",
      "styles": "",
      "state": {
        "collapsed": false,
        "visible": true,
        "selected": true
      },
      "name": "div",
      "type": "box",
      "text": "",
      "props": {
        "class": "bg-gradient-to-br from-purple-800 to-blue-700 h-screen flex items-center justify-center",
        "x-data": "{\n    text: 'Alpine Animation',\n    char: -1,\n    animate() {\n      let timer = setInterval(() => {\n        this.char++;\n        if (char == this.text.length) {\n          clearInterval(timer);\n          timer = null;\n          return;\n        }\n      }, 50);\n    }\n}",
        "x-init": "animate()"
      },
      "children": [
        {
          "tag": "div",
          "id": "wrvhn16jp",
          "styles": "",
          "state": {
            "collapsed": false,
            "visible": true,
            "selected": false
          },
          "name": "div",
          "type": "box",
          "text": "",
          "children": [
            {
              "tag": "h1",
              "id": "v8gktj2yv",
              "styles": "",
              "state": {
                "collapsed": false,
                "visible": true,
                "selected": false
              },
              "name": "h1",
              "type": "text",
              "text": "",
              "props": {
                "class": "h-14 text font-bold text-7xl tracking-wide text-white"
              },
              "children": [
                {
                  "tag": "template",
                  "id": "t8xt4fpdc",
                  "styles": "",
                  "state": {
                    "collapsed": false,
                    "visible": true,
                    "selected": false
                  },
                  "name": "template",
                  "type": "box",
                  "text": "",
                  "props": {
                    "x-for": "(c, i) in text.split('')"
                  },
                  "children": [
                    {
                      "tag": "span",
                      "id": "pvwdmjaj2",
                      "styles": "",
                      "state": {
                        "collapsed": false,
                        "visible": true,
                        "selected": false
                      },
                      "name": "span",
                      "type": "text",
                      "text": "",
                      "props": {
                        "x-text": "c",
                        "class": "opacity-0 transition delay-75 ease-in",
                        ":class": "{'opacity-100':char>=i}"
                      }
                    }
                  ]
                }
              ]
            },
            {
              "tag": "button",
              "id": "rxi870yqx",
              "styles": "",
              "state": {
                "collapsed": false,
                "visible": true,
                "selected": false
              },
              "name": "button",
              "type": "text",
              "text": "Play animation",
              "props": {
                "class": "block bg-white mx-auto mt-8 px-4 py-1 rounded-lg",
                "@click": "char = -5"
              },
              "children": []
            }
          ]
        }
      ]
    }
  ],
  "logo": "data:image/svg+xml;base64,",
  "lang": "en",
  "dark": true,
  "previewDark": true,
  "pwa": false,
  "activePanel": "layers"
}