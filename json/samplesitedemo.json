{
  "name": "Sample site demo",
  "version": "0.0.1",
  "title": "Sample site demo",
  "description": "Sample site demo",
  "author": "Polyrise",
  "url": "https://michaelsboost.com/",
  "meta": "",
  "libraries": [],
  "css": {
    "rootVariables": {
      "--primary-background": "#0172ad",
      "--navbar-background-color": "#5360b4",
      "--navbar-hover-background-color": "#7d1e86",
      "--feature-background-color": "#fff",
      "--feature-border-color": "#ccc",
      "--feature-border-width": "1px",
      "--footer-background-color": "#333c4e",
      "--footer-color": "#fff",
      "--body-background": "#eee",
      "--body-color": "#1c1c1c",
      "--feature-gap": "1rem",
      "--transition": "0.2s ease-in-out"
    },
    "styles": {
      "html": {
        "base": {
          "font-family": "Arial, sans-serif",
          "line-height": "1.6"
        }
      },
      "body": {
        "base": {
          "margin": "0",
          "padding": "0",
          "color": "var(--body-color)",
          "background": "var(--body-background)"
        }
      },
      ".navbar": {
        "base": {
          "--bg-color": "var(--navbar-background-color)",
          "--hover-bg-color": "var(--navbar-hover-background-color)",
          "--focus-outline": "2px solid blue",
          "background": "var(--bg-color)",
          "color": "#fff",
          "padding": "1rem",
          "text-align": "center",
          "position": "fixed",
          "top": "0",
          "left": "0",
          "right": "0",
          "transition": "var(--transition)"
        },
        "pseudos": []
      },
      ".hero": {
        "base": {
          "display": "grid",
          "background-color": "#13171f",
          "color": "#fff",
          "padding": "4rem 2rem",
          "text-align": "center",
          "place-items": "center",
          "height": "100vh"
        }
      },
      ".container": {
        "base": {
          "margin": "0"
        }
      },
      ".hero h1": {
        "base": {
          "font-size": "2rem",
          "margin": "0"
        }
      },
      ".btn": {
        "base": {
          "cursor": "pointer",
          "padding": "0.75rem 1rem",
          "line-height": "1.5",
          "font-size": "1rem",
          "font-family": "Arial",
          "font-weight": "400",
          "text-align": "center",
          "width": "140px",
          "height": "50px",
          "border-radius": "0.25rem",
          "border": ".0625rem solid var(--primary-background)",
          "color": "#ffffff",
          "background": "var(--primary-background)",
          "transition": "var(--transition)"
        },
        "pseudos": [
          {
            "selector": ":hover",
            "styles": {
              "background": "#0688fa"
            }
          }
        ]
      },
      ".features": {
        "base": {
          "display": "grid",
          "grid-template-columns": "repeat(1, minmax(0, 1fr))",
          "gap": "var(--feature-gap)",
          "padding": "1rem",
          "justify-content": "space-between"
        }
      },
      ".feature": {
        "base": {
          "background-color": "var(--feature-background-color)",
          "border": "var(--feature-border-width) solid var(--feature-border-color)",
          "border-radius": "8px",
          "padding": "1rem",
          "text-align": "center"
        }
      },
      ".footer": {
        "base": {
          "background-color": "var(--footer-background-color)",
          "color": "var(--footer-color)",
          "text-align": "center",
          "padding": "1rem"
        }
      }
    },
    "animations": {},
    "breakpoints": {
      "640px": {
        ".container": {
          "base": {
            "max-width": "640px"
          }
        }
      },
      "768px": {
        ".features": {
          "base": {
            "grid-template-columns": "repeat(3, minmax(0, 1fr))"
          }
        }
      },
      "1024px": {
        ".container": {
          "base": {
            "max-width": "1024px"
          }
        }
      }
    }
  },
  "components": [
    {
      "name": "button",
      "code": "<button class=\"btn\">Click Me!</button>"
    },
